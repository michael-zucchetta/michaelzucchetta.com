# tracking

POST /api/tracker	controllers.TrackerController.trackFirstAccess

#POST /api/login		controllers.UserLoginController.createUser
#GET /api/login		controllers.UserLoginController.getLogin
#GET /api/auth		controllers.UserLoginController.index


